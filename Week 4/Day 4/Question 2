#include <stdio.h>

int main()
{   
    const int a=10;     
    int *p;            
    p=&a;               
     
    printf("Before changing the value of a: %d",a);
     
    *p=20;
     
    printf("\nAfter  changing the value of a: %d",a); 
    printf("\nvalue has changed.");
     
    return 0;
}