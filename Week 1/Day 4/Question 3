#include <stdio.h>

int main() {

    int a, b;

    unsigned long long fact = 1;

    printf("Enter an integer: ");

    scanf("%d", &a);

    if (a < 0)

        printf("Factorial of a negative number does not exist.");

    else {

        for (b = 1; b <= a; ++b) {

            fact *= b;

        }

        printf("Factorial of %d = %llu", a, fact);

    }

    return 0;

}

